/* Carousel
    ================================================== */
/* faster sliding speed */
/*.carousel-inner > .item {
    -webkit-transition: 0.3s ease-in-out left;
    -moz-transition: 0.3s ease-in-out left;
    -o-transition: 0.3s ease-in-out left;
    transition: 0.3s ease-in-out left;
}*/
.carousel,.item,.active {
    height:100%;
}
.carousel .item {
    padding-top: 80px;
}
.carousel {
/*    top: -80px;*/
}
.carousel-inner {
    height:100%;
}
.fill-top {
     width:100%;
    height:100%;
    padding: 0;
    margin: 0;
    background-position:center;
    margin-bottom: -80px;
}
.fill { 
    width:100%;
    height:100%;
    padding: 0;
    margin: 0;
    background-position:center;
    background-size:cover;
}

.carousel-caption {
    font-family: 'AvenirNextNormal', sans-serif;
    text-align: left;
    left: auto;
    max-width: 1500px;
    font-size: 18px;
    right: auto;
    top: 30%;
    text-transform: uppercase;
}
.carousel-caption .btn {
    color: #231f20;
}
.carousel-caption .btn:hover {
    color: #fff;
}
.carousel-caption > h2 {
    font-family: 'AvenirNextDemiBold', sans-serif;
    font-size: 50px;
    letter-spacing: 3px;
}
.carousel-caption > p {
    line-height: 25px;
    letter-spacing: 3px;
    margin: 25px 0;
}

.carousel-inner > .item > img {
    margin: 0 auto;
    height: 50%;
}
.carousel-indicators {
    bottom: 50px;
}
.carousel-indicators li {
    width: 30px;
    margin: 0;
    border-radius: 0;
    height: 4px;
    border: none;
    background: #fff;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)";       /* IE 8 */
    filter: alpha(opacity=50);  /* IE 5-7 */
    -moz-opacity: 0.5;          /* Netscape */
    -khtml-opacity: 0.5;        /* Safari 1.x */
    opacity: 0.5; 
}
.carousel-indicators .active {
    width: 30px;
    border-radius: none;
    height: 4px;
    border: none;
    background: #fff;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";       /* IE 8 */
    filter: alpha(opacity=100);  /* IE 5-7 */
    -moz-opacity: 1;          /* Netscape */
    -khtml-opacity: 1;        /* Safari 1.x */
    opacity: 1; 
}
.scroll-down {
    left: 50%;
    margin-left: -10%;
    padding-left: 0;
    position: absolute;
    text-align: center;
    width: 20%;
    bottom: 0;
    line-height: 50px;
    height: 50px;
    z-index: 8999;
    background: #000;
    text-transform: uppercase;
    font-size: 12px;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=70)"; 
    filter: alpha(opacity=70); 
    -moz-opacity: 0.7;  
    -khtml-opacity: 0.7;
    opacity: 0.7;
}
.scroll-down:hover {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)"; 
    filter: alpha(opacity=80); 
    -moz-opacity: 0.8;  
    -khtml-opacity: 0.8;
    opacity: 0.8;
}
