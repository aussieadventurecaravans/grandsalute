/* Bootstrap Overright
    ================================================== */
.btn-default {
    font-family: 'AvenirNextBold', sans-serif;
    background: #000;
    border: none;
    font-size: 11px;
    text-transform: uppercase;
    border-radius: 0;
    padding: 15px 25px;
    width: 100%;
    color: #fff;
    text-shadow: none;
}
  .btn-default:hover {
      background: #818286;
      color: #fff;
  }  
.btn-primary {
    background: #fff;
    color: #231f20 ;
    border: none;
    font-size: 11px;
    text-transform: uppercase;
    border-radius: 0;
    padding: 15px 25px;
    box-shadow: none;
}
.btn-primary:hover {
    background: #000;
    border: none;
    color: #fff ;
}
.btn-sm, .btn-group-sm > .btn {
    border-radius: 0;
    font-size: 12px;
    line-height: 1.5;
    padding: 8px 10px;
    letter-spacing: 1px;
    
}
.form-control {
    background: #58585b;
    border: none;
    color: #fff;
    border-radius: 0;
}
.searchclear {
    background: #000;
    background-image: url("../img/sprite.png") ;
    background-position: -63px -182px;
    position: absolute;
    right: 0;
    top: 0;
    bottom: 0;
    margin: 6px;
    font-size: 18px;
    cursor: pointer;
    color: #fff;
    padding: 7px 12px;
}
.searchclear:hover {
    background-color: #231f20 ;
}
.btn-file {
  position: relative;
  overflow: hidden;
 background: #bdbec0 none repeat scroll 0 0;
    color: #231f20;
    width: 100%;
    display: inline-block;
}
.btn-file input[type=file] {
  position: absolute;
  top: 0;
  right: 0;
  min-width: 100%;
  min-height: 100%;
  font-size: 100px;
  text-align: right;
  filter: alpha(opacity=0);
  opacity: 0;
  background: red;
  cursor: inherit;
  display: block;
}
input[readonly] {
  background-color: white !important;
  cursor: text !important;
}


