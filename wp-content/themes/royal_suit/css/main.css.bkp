/* keep full widget on smaller screens */
@media (max-width: 767px) { 
	body {
		padding-left: 0;
		padding-right: 0;
	}
}

/* Desktops  */
@media only screen and (max-width : 1200px) {
    .navbar-primary .navbar-nav > li > a {
        padding: 15px 55px;
    }
    .filters label {
        width: 100%;
    }
}

/* Tablets (iPads) */
@media only screen and (max-width : 768px) {
    .dropdown-menu > li > a {
        width: auto;
        
    }
    .navbar-primary .navbar-nav > li > a {
        padding: 15px 35px;
        width: auto;
    }

}

/* Other Small Tablets Portrait */
@media only screen and (max-width : 767px) {
    .logo {
        margin: 10px;
    }
    .carousel-caption > h2 {
        font-size: 40px;
    }
    .fill-top-product {
        margin-bottom: 80px;
    }

}

/* Other Small Tablets Landscaper */
@media only screen and (max-width : 800px) {
    .navbar-primary .navbar-nav > li > a {
        padding: 15px 35px;
    }

}

/* Extra Small Devices, Phones */ 
@media only screen and (max-width : 480px) {
    .logo {
        margin: 10px;
    }
    
    .scroll-down {
        width: 50%;
        margin-left: -25%;
    }
    .carousel-caption > h2 , .product-img-caption > h2 {
        font-size: 30px;
    }

    .featured-products h2, .product-wheretobuy h2, .products-specs h2 {
        font-size: 16px;
        margin: 0 10%;
    }

    .latest-news-home h2 {
        font-size: 18px;
        margin: 0 15%;
    }

    .calltoaction-home .btn-primary {
        width: 90%;
        margin: 10px 0;
    }

}

