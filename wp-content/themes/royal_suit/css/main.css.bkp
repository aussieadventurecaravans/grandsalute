}

html, body, .container-fluid, .row {
    height: 100%;
}
body {
    font-family: 'AvenirNextNormal', sans-serif;
    letter-spacing:2px;
    color: #fff;
}
a {
  color: #fff;
    outline: none;
}
a:active,
a:hover,
